{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/LoginForm.jsx",
    "app/layout.tsx",
    "app/loading.jsx"
, "app/api/auth/[...nextauth]/route.js", "app/api/contact/route.js", "components/ExerciseCardDashboard.jsx", "app/api/catalog/route.js", "lib/getUserIdFromRequest.js"  ],
  "exclude": [
    "node_modules"
  ]
}
